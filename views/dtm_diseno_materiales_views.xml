<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <data>
         <record id="dtm_diseno_materiales_tree" model="ir.ui.view">
            <field name="name">dtm_diseno_materiales_tree</field>
            <field name="model">dtm.diseno.materiales</field>
            <field name="arch" type="xml">
                <tree create="false">
                    <field name="id"></field>
                    <field name="material"></field>
                    <field name="calibre"></field>
                    <field name="largo"></field>
                    <field name="ancho"></field>
                    <field name="disponible"></field>
                    <field name="area"></field>
                </tree>
            </field>
        </record>

        <record id="dtm_diseno_materiales_accion" model="ir.actions.act_window">
            <field name="name">Materiales</field>
            <field name="res_model">dtm.diseno.materiales</field>
            <field name="view_mode">tree</field>
<!--            <field name="view_id" ref="dtm_diseno_realizados_tree" />-->
        </record>


    </data>

</odoo>
